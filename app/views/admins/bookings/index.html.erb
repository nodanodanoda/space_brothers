<div class="midasi">
	<h3 class="midasitext">予約一覧</h3>
</div>

<table class="table table-hover table-inverse">
	<thead>
		<tr>
			<th>購入日時</th>
			<th>購入者</th>
			<th>注文数</th>
			<th>注文ステータス</th>
		</tr>
	</thead>

	<tbody>
		<% @bookings.each do |booking| %>
		<tr>
			<td><a href="<%= admins_booking_path(booking.id) %>"><%= l booking.created_at%></a></td>
			<td><%= booking.customer.name_sei%><%= booking.customer.name_mei%></td>
			<td><%= booking.travel.name%></td>
			<td><%= booking.start_day%><%= booking.end_day%></td>
		</tr>
		<% end %>
	</tbody>
</table>